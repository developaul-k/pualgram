{
  "name": "pualgram",
  "version": "1.0.0",
  "description": "Instagram clone with Express + Prisma + React and React Native",
  "main": "index.js",
  "repository": "https://github.com/wallyPual/pualgram",
  "license": "MIT",
  "engines": {
    "yarn": "1.16.0"
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/runtime": "^7.7.6",
    "aws-sdk": "^2.524.0",
    "copy": "^0.3.2",
    "dotenv": "^7.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.5",
    "graphql-tools": "^4.0.4",
    "graphql-yoga": "^1.17.4",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.5.8",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.1.1",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "prisma": "^1.34.10",
    "prisma-client-lib": "^1.31.1",
    "sharp": "^0.22.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "nodemon": "^1.18.11"
  },
  "scripts": {
    "deploy": "prisma deploy",
    "generate": "prisma generate",
    "prisma": "yarn run deploy && yarn run generate",
    "dev": "nodemon --exec babel-node src/server.js",
    "prebuild": "yarn run prisma",
    "build": "babel src -d build",
    "postbuild": "cd src && npx copy ./api/**/*.graphql ../build/api",
    "start": "node build/server.js"
  },
  "proxy": "http://localhost:4000"
}
